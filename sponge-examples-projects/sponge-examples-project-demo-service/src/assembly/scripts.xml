<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
    <id>scripts</id>
    <formats>
        <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
            <directory>sponge</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>*</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>../../sponge-tensorflow/examples/tensorflow/mnist/python</directory>
            <outputDirectory>python</outputDirectory>
            <includes>
                <include>*</include>
            </includes>
            <excludes>
                <exclude>__pycache__/**</exclude>
            </excludes>
        </fileSet>
        <fileSet>
            <directory>../../sponge-tensorflow/examples/tensorflow/mnist/data</directory>
            <outputDirectory>data</outputDirectory>
            <includes>
                <include>*</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>